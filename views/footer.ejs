        margin-bottom: 0;
    }
    
    .mns a:hover {
        background-color: rgb(235, 68, 68);
        padding: 5px;
        border-radius: 5px;
        transition-property: all;
        transition-duration: .3s;
        transition-timing-function: ease-out;
    }
    
    .frsify-footer ul a {
        color: inherit;
        text-decoration: none;
        opacity: 1;
    }
    
    .frsify-footer ul a:hover {
        opacity: 1;
    }
    
    @media (max-width:767px) {
        .frsify-footer .item:not(.social) {
